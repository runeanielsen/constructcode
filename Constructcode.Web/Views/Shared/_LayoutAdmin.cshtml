<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="content-language" content="en">
    <meta http-equiv="Cache-control" content="public">
    <meta name="description" content="@ViewData["Description"]">

    <title>@ViewData["Title"] - Construct Code</title>

    <link async href="~/site.min.css" asp-append-version="true" rel="stylesheet" />
</head>
<body>
    <div ng-app="app">
        @RenderBody()
    </div>

    <script src="~/lib-admin.min.js" asp-append-version="true"></script>
    <script src="~/lib.min.js" asp-append-version="true"></script>
    <script src="~/site.min.js" asp-append-version="true"></script>

    @RenderSection("scripts", false)
</body>
</html>